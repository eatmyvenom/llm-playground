{
  "name": "langchain-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "node --watch --loader=@swc-node/register/esm src/index.ts",
    "openai": "node --watch --loader=@swc-node/register/esm src/openaiServer.ts",
    "build": "swc src -d dist --source-maps",
    "start": "node dist/index.js",
    "start:openai": "node dist/openaiServer.js",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@langchain/community": "npm:@langchain/community@next",
    "@langchain/core": "npm:@langchain/core@next",
    "@langchain/langgraph": "1.0.0-alpha.3",
    "@langchain/openai": "npm:@langchain/openai@next",
    "dotenv": "^17.2.2",
    "fastify": "^5.6.1",
    "langchain": "npm:langchain@next",
    "zod": "^3.23.0"
  },
  "optionalDependencies": {
    "@anatine/zod-decorators": "^1.0.0"
  },
  "devDependencies": {
    "@swc-node/register": "^1.11.1",
    "@types/node": "^20.11.0"
  }
}
